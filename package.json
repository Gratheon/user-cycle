{
  "name": "user-cycle",
  "version": "1.0.0",
  "description": "User registration, billing, deletion, management",
  "dependencies": {
    "@apollo/federation": "0.38.1",
    "@aws-sdk/client-ses": "3.980.0",
    "@databases/mysql": "7.0.0",
    "@gratheon/log-lib": "^2.0.2",
    "@sentry/node": "10.37.0",
    "apollo-server-core": "3.13.0",
    "apollo-server-fastify": "3.13.0",
    "axios": "1.13.4",
    "clarifai-nodejs-grpc": "11.11.0",
    "cross-fetch": "4.0.0",
    "deep-email-validator": "0.1.21",
    "fast-safe-stringify": "2.1.1",
    "fastify": "3.29.5",
    "fastify-cookie": "5.7.0",
    "fastify-raw-body": "3.2.0",
    "graphql": "15.8.0",
    "jwt-encode": "1.0.1",
    "sha1": "1.1.1",
    "stripe": "10.15.0",
    "uuid": "9.0.1"
  },
  "scripts": {
    "build": "tsc -p ./src/config/tsconfig.json",
    "typecheck": "tsc -p ./src/config/tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "ENV_ID=dev nodemon app/user-cycle.js",
    "start": "ENV_ID=prod node app/user-cycle.js",
    "dev:ts": "ENV_ID=dev ts-node-dev --respawn --transpile-only --prefer-ts --exit-child src/user-cycle.ts",
    "dev-in-docker": "ts-node-dev --respawn --transpile-only --prefer-ts --exit-child --project src/config/tsconfig.json src/user-cycle.ts",
    "prepare": "husky || true"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gratheon/user-cycle.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gratheon/user-cycle/issues"
  },
  "homepage": "https://github.com/gratheon/user-cycle#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.11",
    "source-map-support": "0.5.21",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsc-watch": "6.0.4",
    "typescript": "5.2.2"
  },
  "overrides": {
    "deep-email-validator": {
      "axios": "1.13.4"
    },
    "fastify": {
      "light-my-request": {
        "cookie": "0.7.2"
      }
    },
    "fast-xml-parser": "5.3.4"
  }
}
